  interhash = {2c46ed2a31118023a67663a3063e44ba},
  intrahash = {961ea1daf4744be7f0738962b308c75c},
  keywords = {aligned aligned-project group_aksw kilt kilt_publications klimek mmoon ontolex sdw},
  pages = {68--73},
  publisher = {CEUR-WS.org},
  series = {{CEUR} Workshop Proceedings},
  timestamp = {2018-07-31T03:06:52.000+0200},
  title = {Proposing an OntoLex-MMoOn Alignment: Towards an Interconnection of two Linguistic Domain Models},
