  keywords = {2016 SIMBA dbtrends group_aksw hassan lehmann marx mole ngonga openqa projecthobbit rautenberg smart zaveri},
  timestamp = {2018-07-31T03:06:58.000+0200},
  title = {{DBtrends} : {P}ublishing and {B}enchmarking {RDF} {R}anking {F}unctions},
