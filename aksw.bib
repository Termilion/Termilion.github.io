  interhash = {d2cbcefcf73d302e8a28b32a1ff482ad},
  intrahash = {a11b90f293d789ad70eed37a9556d31b},
  keywords = {2015 aksw defacto esteves lehmann ngonga rene speck},
  note = {Semantic Web Challenge, International Semantic Web Conference 2015},
  timestamp = {2018-07-31T03:07:08.000+0200},
  title = {DeFacto - A Multilingual Fact Validation Interface},
