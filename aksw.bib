  interhash = {0918df367910dce80d220f6ac428fd59},
  intrahash = {8225ebcc3c8f29f053b7684d47dd66c7},
  keywords = {2011 SIMBA boa event_iswc event_wekex gerber group_aksw ngonga},
  owner = {gerb},
